<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <type name="Magento\Framework\Console\CommandListInterface">
        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="Vml\CustomerImport\Console\Command\CustomerImportCommand" xsi:type="object">Vml\CustomerImport\Console\Command\CustomerImportCommand</item>
            </argument>
        </arguments>
    </type>
    <type name="Vml\CustomerImport\Model\CustomerImport">
        <arguments>
            <argument name="profiles" xsi:type="array">
                <item name="sample-csv" xsi:type="object">Vml\CustomerImport\Model\Import\CsvImport</item>
                <item name="sample-json" xsi:type="object">Vml\CustomerImport\Model\Import\JsonImport</item>
            </argument>
        </arguments>
    </type>

    <preference for="Vml\CustomerImport\Api\CustomerImportInterface" type="Vml\CustomerImport\Model\CustomerImport" />
    <preference for="Magento\Framework\Filesystem\DriverInterface" type="Magento\Framework\Filesystem\Driver\File"/>
</config>
